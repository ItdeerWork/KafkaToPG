{
  "kafka": {
    "bootstrapServers": "192.168.1.100:9092",
    "groupIdSuffix": "GROUP_ID_PDI",
    "autoCommitIntervalMs": "1000",
    "maxPollRecords": "100000",
    "fetchMaxBytes": "52428800",
    "fetchMaxWaitMs": "1000",
    "sessionTimeoutMs": "10000",
    "heartbeatIntervalMs": "3000",
    "maxPollIntervalMs": "60000",
    "autoOffsetReset": "earliest",
    "enableAutoCommit": "false",
    "keyDeserializerClass": "org.apache.kafka.common.serialization.StringDeserializer",
    "valueDeserializerClass": "org.apache.kafka.common.serialization.StringDeserializer"
  },
  "datasource": [
    {
      "topicName": "fengtai",
      "table": "fengtai",
      "frequency": "20",
      "duration": "6",
      "threads": "1",
      "mapping": "tagname:text,tagvalue:decimal,pits:timestamp,loadtime:timestamp"
    }
  ]
}